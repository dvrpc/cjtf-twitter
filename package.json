{
  "private": true,
  "name": "cjtf-twitter",
  "version": "0.0.0",
  "license": "CC0-1.0",
  "scripts": {
    "build": "preact build --no-esm",
    "serve": "sirv build --port 8080 --cors --single --gzip --brotli",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "copy-webpack-plugin": "^6.2.1",
    "eslint": "^6.0.1",
    "eslint-config-synacor": "^3.0.4",
    "husky": "^4.3.0",
    "preact-cli": "^3.0.0",
    "preconnect-html-webpack-plugin": "^1.0.0",
    "pretty-quick": "^3.1.0",
    "sirv-cli": "^1.0.3"
  },
  "dependencies": {
    "@babel/core": "^7.12.3",
    "@emotion/babel-plugin-jsx-pragmatic": "^0.1.5",
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "lazysizes": "^5.2.2",
    "preact": "^10.1.0",
    "preact-render-to-string": "^5.1.2",
    "twin.macro": "^1.11.1",
    "twitter-text": "^3.1.0"
  },
  "babelMacros": {
    "twin": {
      "config": "tailwind.config.js",
      "preset": "emotion",
      "debugProp": true,
      "debugPlugins": false,
      "debug": false
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    "> 1% in US",
    "IE 11"
  ]
}
